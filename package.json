{
  "name": "dgw-website",
  "version": "0.0.1",
  "description": "Donkey's Gone Wild poker league website",
  "productName": "Donkey's Gone Wild",
  "author": "Roger Hopkins <jrhopkins83@gmail.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue ./",
    "icons": "icongenie generate -i icon.png --skip-trim --theme-color ac4745",
    "deploy-test-pwa": "QENV=staging quasar build -m pwa && firebase deploy -P staging",
    "deploy-prod-pwa": "QENV=production quasar build -m pwa && firebase deploy -P prod",
    "emulators1": "firebase emulators:start --import=dir/base1",
    "emulators2": "firebase emulators:start --import=dir/base2",
    "emulators3": "firebase emulators:start --import=dir/base3",
    "emulators4": "firebase emulators:start --import=dir/base4",
    "test": "jest ./spec --detectOpenHandles",
    "dev": "QENV=staging quasar dev",
    "staging": "QENV=staging quasar dev",
    "prod": "QENV=production quasar dev",
    "deploy-to-staging": "firebase use default && QENV=staging quasar build && firebase deploy",
    "deploy-to-production": "firebase use production && QENV=production quasar build && firebase deploy"
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "cropperjs": "^1.5.9",
    "emoji-mart-vue": "^2.6.6",
    "firebase": "^7.24.0",
    "pica": "^6.1.1",
    "quasar": "^1.0.0",
    "v-money": "^0.8.1",
    "vuedraggable": "^2.24.3",
    "vuelidate": "^0.7.6",
    "vuexfire": "^3.2.4",
    "web-push": "^3.4.4"
  },
  "devDependencies": {
    "@quasar/app": "^2.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.1.2",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "workbox-webpack-plugin": "^5.1.4"
  },
  "browserslist": [
